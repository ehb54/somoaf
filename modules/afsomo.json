{
    "submitpolicy" : "all",
    "label" : "US-SOMO AlphaFold Hydrodynamic Database",
    "noreset" : "true",
    "moduleid" : "afsomo",
    "executable" : "afsomo.php",
    "hoverhighlight" : "true",
    "panels" : [
        {
            "root" : {
                "label" : [
                    1,
                    1
                ],
                "data" : [
                    1,
                    2
                ],
                "gap" : "1px",
                "align" : "left",
                "size" : [
                    1,
                    "auto"
                ]
            }
        },
        {
            "header" : {
                "data" : [
                    2,
                    1
                ],
                "label" : [
                    1,
                    1
                ],
                "align" : "center",
                "gap" : "1px",
                "size" : [
                    "auto",
                    "auto"
                ],
                "location" : [
                    "next",
                    "full"
                ]
            }
        },
        {
            "body" : {
                "location" : [
                    "next",
                    "full"
                ],
                "size" : [
                    "auto",
                    [
                        1,
                        1
                    ]
                ]
            }
        },
        {
            "footer" : {
                "align" : "center",
                "size" : [
                    "auto",
                    "auto"
                ],
                "location" : [
                    "next",
                    "full"
                ],
                "data" : [
                    2,
                    1
                ],
                "label" : [
                    1,
                    1
                ]
            }
        },
        {
            "inputpanel" : {
                "parent" : "body",
                "location" : [
                    "next",
                    "full"
                ],
                "align" : "left",
                "size" : [
                    "auto",
                    [1,1]
                ]
            }
        },
        {
            "resultpanel" : {
                "align" : "left",
                "size" : [
                    "auto",
                    [ 1, 1 ]
                ],
                "location" : [
                    "next",
                    "full"
                ],
                "data" : [
                    1,
                    2
                ],
                "parent" : "body",
                "repeats" : {
                    "indent" : "10px"
                },
                "label" : [
                    1,
                    1
                ]
            }
        }
    ],
    "fields" : [
        {
            "role" : "input",
            "label" : "<h4>US-SOMO-AF</h4>",
            "type" : "label",
            "id" : "label_00"
            ,"marginbottom"    : "0px"
            ,"layout" : {
                "parent" : "header"
            }
        },
        {
            "role" : "input",
            "label" : "<h5><a target=_blank href=http://somo.aucsolutions.com>US-SOMO</a> <a target=_blank href=https://link.springer.com/article/10.1007%2Fs00249-018-1296-0>Hydrodynamic, Structural</a> and <a target=_blank href=https://www.mpibpc.mpg.de/sesca>SESCA CD</a> Calculations on <a target=_blank href=https://alphafold.ebi.ac.uk>AlphaFold</a> Predicted Structures</h5>",
#            "label" : "<h5>US-SOMO Hydrodynamic, Structural and SESCA CD Calculations on AlphaFold Predicted Structures</h5>",
            "type" : "label",
            "id" : "label_0"
            ,"marginbottom"    : "45px"
            ,"layout" : {
                "parent" : "header"
            }
        },
        {
            "role" : "input",
            "id" : "searchkey",
            "help" : "Enter an AlphaFold predicted UniProt accession code or the start of a code and click Search",
            "type" : "text",
            "maxlen" : 30,
            "label" : "UniProt accession",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "inputpanel"
            }
        },
        {
           "role"        : "input"
           ,"id"         : "b_submit"
           ,"type"       : "button"
            ,"buttontext" : "Search"
            ,"margin"    : "1px"
           ,"layout"    : {
               "parent"    : "inputpanel"
#               ,"location" : [ row, column ] (optional - defaults to [ "next", "next" ] )
#               ,"label"    : [ row, column ] (optional - inherits )
#               ,"data"     : [ row, column ] (optional - inherits )
#               ,"align"    : "right"         (optional - inherits )
#               ,"repeats"  : {}              (optional - inherits - only used for repeater fields)
           }
        },
        {
            "role" : "output",
            "label" : "AlphaFold model name",
            "type" : "text",
            "help" : "This is the full name from the AlphaFold database",
            "id" : "name",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "help" : "As reported in the AlphaFold PDB TITLE information",
            "id" : "title",
            "type" : "textarea",
            "role" : "output",
            "label" : "Title",
            "rows" : 3,
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "help" : "As reported in the AlphaFold PDB SOURCE information",
            "id" : "source",
            "type" : "textarea",
            "role" : "output",
            "label" : "Source",
            "rows" : 5,
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "help" : "Identified from the UniProt&apos;s &quot;PTM/Processing Signal peptide&quot;<br>All results are computed with these residues removed.",
            "id" : "sp",
            "type" : "text",
            "role" : "output",
            "label" : "Signal peptide removed",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "help" : "The date from the structure in the AlphaFold database",
            "id" : "afdate",
            "type" : "text",
            "label" : "AlphaFold date",
            "role" : "output",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "label" : "Mean confidence",
            "role" : "output",
            "help" : "Mean per-residue confidence is computed from the structure&apos;s AlphaFold residue confidence",
            "type" : "text",
            "id" : "afmeanconf",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "help" : "The date hydrodynamic and structural calculations were performed",
            "id" : "somodate",
            "type" : "text",
            "label" : "Hydrodynamic, structural and CD calculations date",
            "role" : "output",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "label" : "Molecular mass [Da]",
            "role" : "output",
            "id" : "mw",
            "help" : "The atomic mass",
            "type" : "text",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "id" : "psv",
            "help" : "Computed from the sequence",
            "type" : "text",
            "role" : "output",
            "label" : "Partial specific volume <b><i><span style=text-decoration:overline>&nu;<span></i></b> &nbsp;[cm<sup>3</sup>/g]",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "label" : "Translational diffusion coefficient <b><i>D<sup>0</sup><sub>20,w</sub></i></b> &nbsp;[F]",
            "role" : "output",
            "help" : "Computed from the theoretically hydrated structure, [F] (Ficks) = 10<sup>-7</sup> [cm/sec<sup>2</sup>]",
            "id" : "Dtr",
            "type" : "text",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "label" : "Sedimentation coefficient <b><i>s<sup>0</sup><sub>20,w</sub></i></b> &nbsp;[S]",
            "role" : "output",
            "type" : "text",
            "help" : "Computed from the translational diffusion coefficient, molecular mass and partial specific volume",
            "id" : "S",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "label" : "Stokes radius <b><i>R<sub>S</sub></i></b> &nbsp;[nm]",
            "role" : "output",
            "type" : "text",
            "help" : "Computed from the translational diffusion coefficient",
            "id" : "Rs",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "type" : "text",
            "help" : "Computed from the theoretically hydrated structure",
            "id" : "Eta",
            "label" : "Intrinsic viscosity <b>[<i>&eta;</i>]</b> &nbsp;[cm<sup>3</sup>/g]",
            "role" : "output",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "type" : "text",
            "help" : "Computed from the atomic structure",
            "id" : "Rg",
            "role" : "output",
            "label" : "Radius of gyration <b><i>R<sub>g</sub></i></b> &nbsp;[&#8491;]",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "label" : "Maximum extension X [nm]",
            "role" : "output",
            "help" : "The principle axis aligned structure&apos;s maximum extension in the X dimension",
            "type" : "text",
            "id" : "ExtX",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "role" : "output",
            "label" : "Maximum extension Y [nm]",
            "help" : "The principle axis aligned structure&apos;s maximum extension in the Y dimension",
            "id" : "ExtY",
            "type" : "text",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "label" : "Maximum extension Z [nm]",
            "role" : "output",
            "type" : "text",
            "help" : "The principle axis aligned structure&apos;s maximum extension in the Z dimension",
            "id" : "ExtZ",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "type" : "text",
            "help" : "The percent of peptide bonds identified by DSSP, implemented in UCSF Chimera, as &alpha;-Helix",
            "id" : "helix",
            "label" : "% Helix",
            "role" : "output",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "label" : "% Sheet",
            "role" : "output",
            "help" : "The percent of peptide bonds identified by DSSP, implemented in UCSF Chimera, as &beta;-Sheet",
            "type" : "text",
            "id" : "sheet",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "id" : "links",
            "help" : "Links to external sites",
            "type" : "html",
            "label" : "External links",
            "role" : "output",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "label" : "Downloads",
            "role" : "output",
            "help" : "Notes<br>PDB and mmCIF - signal peptide removed (if present); disulphide bonds, &alpha;-Helix and &Beta;-Sheet annotated<br>mmCIF was produced using RCSB&apos;s MAXIT Suite from the PDB<br>All zip&apos;d - zip archive for this model<br>All txz&apos;d - tar xz archive for this model",
            "id" : "downloads",
            "type" : "html",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "role" : "output",
            "label" : "SAXS P(r)",
            "help" : "The P(r) is computed on the structure for comparison with SAXS derived data and is normalized by molecular mass",
            "type" : "plotly",
            "id" : "prplot",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "role" : "output",
            "label" : "CD (Circular Dichroism) spectrum",
            "help" : "The CD is computed on the structure for comparison with CD experimental data",
            "type" : "plotly",
            "id" : "cdplot",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        },
        {
            "width" : "100%",
            "label" : "Structure",
            "help"  : "Coloring is by per residue confidence<br>Blues are highest confidence, reds lowest<br>See the AlphaFold external link for confidence details<br>Note - AlphaFold viewer has modified coloring",
            "role" : "output",
            "jsmoladd" : "ribbon only; color temperature",
            "id" : "struct",
            "type" : "atomicstructure",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        }
    ]
}
