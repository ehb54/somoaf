{
    "moduleid"      : "afsomo"
    ,"label"        : "US-SOMO AlphaFold Hydrodynamic Calculations"
    ,"executable"   : "afsomo.php"
    ,"submitpolicy" : "all"
    ,"submit_label" : "Search"
    ,"noreset"      : "true"
    ,"fields" : [
        {
            "role"   : "input"
            ,"id"    : "label_0"
            ,"type"  : "label"
            ,"label" : "<center><big><a target=_blank href=http://somo.aucsolutions.com>US-SOMO</a> <a target=_blank href=https://alphafold.ebi.ac.uk>AlphaFold</a> Hydrodynamic Calculations</big></center>"
        }
        ,{
            "role"   : "input"
            ,"id"     : "searchkey"
            ,"label"  : "UniProt accession"
            ,"type"   : "text"
            ,"maxlen" : 30
        }
        ,{
            "role"   : "output"
            ,"id"    : "name"
            ,"label" : "AlphaFold Model name"
            ,"type"  : "text"
        }
        ,{
            "role"   : "output"
            ,"id"    : "source"
            ,"label" : "Source"
            ,"rows"  : 5
            ,"type"  : "textarea"
            ,"help"  : "This information is as reported in the AlphaFold PDB SOURCE information"
        }
        ,{
            "role"   : "output"
            ,"id"    : "sp"
            ,"label" : "Signal protein cut"
            ,"type"  : "text"
            ,"help"  : "Residues cut from the AlphaFold structure prior to hydrodynamic calculations"
        }
        ,{
            "role"   : "output"
            ,"id"    : "afdate"
            ,"label" : "AlphaFold date"
            ,"type"  : "text"
        }
        ,{
            "role"   : "output"
            ,"id"    : "mw"
            ,"label" : "Molecular mass [Da]"
            ,"type"  : "text"
        }
        ,{
            "role"   : "output"
            ,"id"    : "psv"
            ,"label" : "Partial specific volume [cm^3/g]"
            ,"type"  : "text"
        }
        ,{
            "role"   : "output"
            ,"id"    : "S"
            ,"label" : "Sedimentation coefficient s [S]"
            ,"type"  : "text"
        }
        ,{
            "role"   : "output"
            ,"id"    : "Dtr"
            ,"label" : "Translational diffusion coefficient D [cm/sec^2]"
            ,"type"  : "text"
        }
        ,{
            "role"   : "output"
            ,"id"    : "Rs"
            ,"label" : "Stokes radius [nm]"
            ,"type"  : "text"
        }
        ,{
            "role"   : "output"
            ,"id"    : "Eta"
            ,"label" : "Intrinsic viscosity [cm^3/g]"
            ,"type"  : "text"
        }
        ,{
            "role"   : "output"
            ,"id"    : "Rg"
            ,"label" : "Radius of gyration (+r) [A]"
            ,"type"  : "text"
            ,"help"  : "The radius of gyration is computed on the atomic structure"
        }
        ,{
            "role"   : "output"
            ,"id"    : "ExtX"
            ,"label" : "Maximum extensions X [nm]"
            ,"type"  : "text"
            ,"help"  : "The principle axis aligned X dimension"
        }
        ,{
            "role"   : "output"
            ,"id"    : "ExtY"
            ,"label" : "Maximum extensions Y [nm]"
            ,"type"  : "text"
            ,"help"  : "The principle axis aligned Y dimension"
        }
        ,{
            "role"   : "output"
            ,"id"    : "ExtZ"
            ,"label" : "Maximum extensions Z [nm]"
            ,"type"  : "text"
            ,"help"  : "The principle axis aligned Z dimension"
        }
        ,{
            "role"   : "output"
            ,"id"    : "links"
            ,"label" : "External links"
            ,"type"  : "html"
            ,"help"  : "Links to external sites"
        }
        ,{
            "role"   : "output"
            ,"id"    : "downloads"
            ,"label" : "Downloads"
            ,"type"  : "html"
            ,"help"  : "Note - the PDB and CIF have signal peptides cut (if present) & SS bonds annotated<br>All zip&apos;d - zip archive of all<br>All txz&apos;d - tar xz archive of all"
        }
        ,{
            "role"   : "output"
            ,"id"    : "prplot"
            ,"type"  : "plotly"
            ,"label" : ""
        }
    ]
}
